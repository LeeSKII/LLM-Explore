    "import requests\n",
    "def search_ddg(query):\n",
    "    url = \"https://api.duckduckgo.com/\"\n",
    "    params = {\"q\": query, \"format\": \"json\"}\n",
    "    response = requests.get(url, params=params)\n",
    "    return response.json()\n",
    "results = search_ddg(\"time\")\n",
    "print(results[\"RelatedTopics\"])"
   ]
